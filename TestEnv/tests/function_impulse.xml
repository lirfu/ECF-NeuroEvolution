<ECF>
    <Algorithm>
        <SteadyStateTournament>
            <Entry desc="" key="tsize">3</Entry>
        </SteadyStateTournament>
    </Algorithm>
    <Genotype>
        <Tree>
            <Entry key="maxdepth">9</Entry>
            <Entry key="mindepth">1</Entry>
            <Entry key="functionset">P S</Entry>
            <Entry key="terminalset">E</Entry>
        </Tree>
    </Genotype>

    <Registry>
        <Entry key="nev.problem">function</Entry> <!-- xor, function, wine -->
        <Entry key="nev.problem.function">impulse</Entry> <!-- onedim, rosenbrock, impulse -->
        <Entry key="nev.hiddenFunction">sigmoid</Entry> <!-- linear, sigmoid -->
        <Entry key="nev.outputFunction">linear</Entry>
        <Entry key="nev.learningRate">1e-3</Entry>
        <Entry key="nev.minLoss">1e-9</Entry>
        <Entry key="nev.maxIterations">1000000</Entry>

        <Entry key="population.size">20</Entry>
        <Entry key="mutation.indprob">0.35</Entry>

        <Entry key="term.fitnessval">1e-9</Entry>
        <Entry key="term.maxgen">30</Entry>
        <!-- <Entry key="term.maxtime">0</Entry> -->

        <Entry key="log.level">3</Entry>
        <Entry key="log.filename">log.txt</Entry>
    </Registry>

</ECF>

